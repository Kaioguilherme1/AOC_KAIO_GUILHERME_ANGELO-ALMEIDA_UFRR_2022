vsim work.instrucoes
# vsim work.instrucoes 
# Start time: 17:57:16 on Mar 03,2022
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading ieee.numeric_std(body)
# Loading work.instrucoes(main)
wave create -driver freeze -pattern clock -initialvalue 0 -period 1000ps -dutycycle 50 -starttime 0ns -endtime 1000ns sim:/instrucoes/clk
# Editable Wave items are not compatible with the wave window's standard undo-redo.  Standard undo-redo has been disabled.
add wave -position insertpoint  \
sim:/instrucoes/loop_func
wave create -driver freeze -pattern constant -value 01 -range 1 0 -starttime 0ps -endtime 1000ps sim:/instrucoes/loop_func
add wave -position insertpoint  \
sim:/instrucoes/op_code
add wave -position insertpoint  \
sim:/instrucoes/reg1
add wave -position insertpoint  \
sim:/instrucoes/reg2
add wave -position insertpoint  \
sim:/instrucoes/func
add wave -position insertpoint  \
sim:/instrucoes/indice
run
